{
    "type": "object",
    "properties": {
      "dimmer": {
        "descriptions": "Proportion of requests served with optional content.",
        "type": "number"
      },
      "servers": {
        "description": "The number of servers.",
        "type": "integer"
      },
      "max_servers": {
        "description": "The maximum number of servers.",
        "type": "integer"
      },
      "active_servers": {
        "description": "The number of active servers.",
        "type": "integer"
      },
      "utilization": {
        "description": "The server utilization.",
        "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "server_name": {
                        "type": "string"
                    },
                    "utilization_value": {
                        "type": "integer"
                    }
                },
                "required": ["server_name", "utilization_value"]
            }
      },
      "basic_rt": {
        "description": "The response time of requests served without optional content.",
        "type": "float"
      },
      "basic_throughput": {
        "description": "The throughput of requests served without optional content.",
        "type": "float"
      },
      "opt_rt": {
        "description": "The response time of requests served with optional content.",
        "type": "float"
      },
      "opt_throughput": {
        "description": "The throughput of requests served with optional content.",
        "type": "float"
      },
      "arrival_rate": {
        "description": "The number of requests per second received by the web application.",
        "type": "float"
      }
    }
}
